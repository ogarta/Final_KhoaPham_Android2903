<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.svute.appsale">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo_app_sale"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo_app_sale"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppSale">
        <activity
            android:name=".presentation.view.activity.cart.history.CartHistoryActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.view.activity.history.HistoryActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.view.activity.cart.current.CartActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.view.activity.onboard.OnboarDingActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.view.activity.sign_up.SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.view.activity.sign_in.SignInActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.view.activity.splash.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.view.activity.home.HomeActivity"
            android:exported="false" />
    </application>

</manifest>